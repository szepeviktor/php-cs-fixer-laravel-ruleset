{
  "name": "szepeviktor/php-cs-fixer-laravel-ruleset",
  "description": "Provides the ruleset from Laravel Pint",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-filter": "*",
    "friendsofphp/php-cs-fixer": "~3.8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28.1",
    "phpstan/phpstan": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "SzepeViktor\\PhpCsFixer\\Laravel\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "platform": {
      "php": "7.4.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
